{
  "name": "rubiks-cube",
  "version": "1.0.1",
  "description": "Interactive Rubik's Cube using Three.js and React",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "deploy": "node deploy.js",
    "deploy:prod": "node deploy-production.js",
    "deploy:s3": "node deploy-s3.js",
    "serve": "npx serve -s build",
    "log-server": "node log-server.js",
    "monitor-logs": "node monitor-logs.js",
    "dev": "concurrently \"npm run log-server\" \"npm start\"",
    "invalidate-cache": "node invalidate-cloudfront.js",
    "predeploy": "npm test && npm run build"
  },
  "dependencies": {
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.1",
    "three": "^0.160.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@craco/craco": "^7.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "concurrently": "^9.2.1",
    "jest-environment-jsdom": "^30.1.2",
    "mime-types": "^2.1.35",
    "postcss": "^8.5.6",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "nth-check": ">=2.0.1",
    "css-select": ">=4.0.0",
    "svgo": ">=2.0.0"
  }
}
